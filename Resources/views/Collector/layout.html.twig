{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
    <a href="#">
        <img width="26" height="28" src="data:image/png;base64,"/>
            <span>
                Debug Urls
            </span>
    </a>
    {% endset %}

    {% set text %}
        {% for url, label in collector.data.urls %}
            <div class="sf-toolbar-info-piece">
                <a target="_blank" href="{{ url }}" rel="{{ label }}">{{ label }}</a>
            </div>
        {% endfor %}
    {% endset %}

    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': false } %}
{% endblock %}